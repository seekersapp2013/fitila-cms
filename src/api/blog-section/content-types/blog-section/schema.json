{
  "kind": "singleType",
  "collectionName": "blog_sections",
  "info": {
    "singularName": "blog-section",
    "pluralName": "blog-sections",
    "displayName": "Blog Section"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "SectionTitle": {
      "type": "string",
      "required": true,
      "default": "LATEST NEWS"
    },
    "SectionHeader": {
      "type": "string",
      "required": true,
      "default": "Our Blogs."
    },
    "FeaturedBlogs": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::blog.blog"
    }
  }
}
